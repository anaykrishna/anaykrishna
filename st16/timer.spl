[PROCESS_TABLE + 16*[SYSTEM_STATUS_TABLE+1] + 13] = SP;		//Changing of Stack Pointer from usr to kernel
SP = 512 * [PROCESS_TABLE + 16 * [SYSTEM_STATUS_TABLE+1] + 11]; 

backup;

[PROCESS_TABLE + 16*[SYSTEM_STATUS_TABLE+1] + 4] = READY;	//just before scheduler change the current process to READY

call SCHEDULER;

restore;

SP = [PROCESS_TABLE + 16*[SYSTEM_STATUS_TABLE+1] + 13];
[PROCESS_TABLE + 16*[SYSTEM_STATUS_TABLE + 1] + 9] = 0;

ireturn;
